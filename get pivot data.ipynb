   "source": [
    "import pandas as pd\n",
    "cats=pd.read_csv('tags and categories.csv')\n",
    "valid_cats=cats[cats['Exclude'].isnull()]\n",
    "table=pd.pivot_table(valid_cats,values='Tag_Total',index='Contained in',aggfunc='sum')\n",
    "table.sort_values(by='Tag_Total',ascending=False)\n"
   ]
  }
